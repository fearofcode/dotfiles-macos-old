general {
        colors = true
        color_good = "#98971a"
        color_bad = "#cc241d"
        color_degraded = "#fabd2f"
}

order += "wireless _first_"
order += "battery 0"
order += "time"

wireless _first_ {
        format_up = "%essid %quality"
        format_down = "W: down"
}

battery 0 {
        format = "%status %percentage %remaining"
        integer_battery_capacity = true
}

time {
	format="%Y-%m-%d %H:%M:%S"
}

